#!/bin/zsh
${SPARK_HOME}/bin/spark-submit \
--verbose --master spark://besh01:7077 \
--class org.apache.spark.examples.blaze.$1 \
--name "$1" \
--driver-memory 2G \
--driver-cores 1 \
--executor-memory 2G \
--executor-cores 1 \
--num-executors $2 \
--total-executor-cores $2 \
file:///home/xialb/git/spark/examples/target/scala-2.12/jars/spark-examples_2.12-3.2.1-SNAPSHOT.jar \
$3 $4 $5 $6
